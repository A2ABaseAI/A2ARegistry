# UPS Tracking Agent Environment Configuration
# Copy this file to .env and fill in your actual values

# UPS API Configuration
UPS_CLIENT_ID=your-ups-client-id-here
UPS_CLIENT_SECRET=your-ups-client-secret-here
UPS_ACCOUNT_NUMBER=your-ups-account-number-here

# UPS API Settings
UPS_API_BASE=https://onlinetools.ups.com
UPS_USE_SANDBOX=false

# Alternative sandbox configuration (uncomment to use)
# UPS_API_BASE=https://wwwcie.ups.com
# UPS_USE_SANDBOX=true

# Application Settings
DEBUG=false
LOG_LEVEL=INFO

# Optional: OpenAI API Key for CrewAI (if using OpenAI models)
# OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Custom model configuration
# CREWAI_MODEL=gpt-4o-mini
# CREWAI_TEMPERATURE=0.1
